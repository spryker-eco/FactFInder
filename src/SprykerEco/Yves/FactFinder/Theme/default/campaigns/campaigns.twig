{% for campaign in campaigns %}
    {% if campaign.hasRedirect %}
        {% include '@FactFinder/campaigns/partials/redirect.twig' with {
            campaign: campaign,
        } %}
    {% endif %}
    {% if campaign.hasPushedProducts %}
        {% include '@FactFinder/campaigns/partials/pushed-products.twig' with {
            campaign: campaign,
        } %}
    {% endif %}
{% endfor %}