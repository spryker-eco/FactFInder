{
  "name": "spryker-eco/fact-finder",
  "description": "FactFinder bundle",
  "license": "MIT",
  "require": {
    "spryker-eco/fact-finder-sdk": "@dev",
    "spryker/kernel": "^3.0.0",
    "spryker/session": "^3.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
